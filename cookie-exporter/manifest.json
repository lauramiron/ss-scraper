{
  "manifest_version": 3,
  "name": "Cookie Exporter",
  "version": "1.0",
  "description": "Export cookies (including HttpOnly) as JSON",
  "permissions": ["cookies", "downloads", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {}
}